#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>
#include<string.h>
int main()
{
	int a = 2;
	int b = 2;
	printf("输入的两个数为:");
	scanf("%d%d", &a, &b);
		if (a>=b)
		{
			int i=b;
			for (i = b; i >= 1; i--)
			{
				if (a % i == 0 && b % i == 0)
				{
					printf("%d\n", i);
					break;
				}
			}
		}
		else
		{
			int i = a;
			for (i = a; i >= 1; i--)
			{
				if (a % i == 0 && b % i == 0)
				{
					printf("%d\n", i);
					break;
				}
			}
		}
		return 0;
