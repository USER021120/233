int n(int a)
{
	static int i = 1;
	if ((a % 10) % 2 != 0 && (a % 10) != 0)
	{
		i = i * 10;
		return 1 * (i / 10) + n(a / 10);
	}
	else if ((a % 10) % 2 == 0 && (a % 10) != 0)
	{
		i = i * 10;
		return n(a / 10);
	}
	else
		return 0;
}

int main()
{
	int a = 0;
	scanf("%d", &a);
	printf("%d", n(a));
	/*int num = 0;
	int i = 1;
	while ((a % 10) != 0)
	{
		if ((a % 10) % 2 != 0)
			num += 1 * i;
		i = i * 10;
		a = a / 10;
	}
	printf("%d", num);*/
