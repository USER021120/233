#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>
#include<string.h>
int main()
{
	int a = 0;
	int b = 0;
	printf("输入的两个数为:");
	scanf("%d%d", &a, &b);
		if (a>=b)
		{
			int i=b;
			while(i>=1)
			{
				if (a % i == 0 && b % i == 0)
				{
					printf("%d\n", i);
					break;
				}
				else
					i--;
			}
		}
		else
		{
			int i = a;
			while (i >= 1)
			{
				if (a % i == 0 && b % i == 0)
				{
					printf("%d\n", i);
					break;
				}
				else
					i--;
			}
		}
		return 0;
}
